datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model User{
 id Int @id @default(autoincrement()) 
 email String @unique
 username String @db.VarChar(100)
 password String 
 isAdmin
}
model Comment{
  id Int @id @default(autoincrement())
  text   String
  createdAt DateTime @default(now())
}

